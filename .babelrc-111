{
  "presets": ["es2015", "react", "stage-2"],
  "plugins": [
    ["module-resolver", {
        "root": ["./src/client/app"],
        "alias": {
          "components": "/components",
          "common": "/components/common",
          "containers": "/containers",
          "lib": "/lib",
          "css": "/css"
        },
        "extensions":[".js", ".jsx", ".es", ".es6",".css"]
      }
    ]
  ]
}
